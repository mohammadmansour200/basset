{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": ["main"],
  "permissions": [
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-mkdir",
    "fs:allow-exists",
    "fs:allow-temp-write",
    "fs:allow-resource-read-recursive",
    {
      "identifier": "fs:scope",
      "allow": ["$TEMP/**/*", "$VIDEO/*", "$AUDIO/*"]
    },
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "args": true,
          "name": "bin/ffmpeg",
          "sidecar": true
        },
        {
          "args": true,
          "name": "bin/ffprobe",
          "sidecar": true
        },
        {
          "args": true,
          "name": "resources/spleeter",
          "sidecar": true
        }
      ]
    },
    {
      "identifier": "shell:allow-spawn",
      "allow": [
        {
          "args": true,
          "name": "bin/ffmpeg",
          "sidecar": true
        },
        {
          "args": true,
          "name": "bin/ffprobe",
          "sidecar": true
        },
        {
          "args": true,
          "name": "resources/spleeter",
          "sidecar": true
        }
      ]
    },
    {
      "identifier": "shell:allow-kill",
      "allow": [
        {
          "args": true,
          "name": "bin/ffmpeg",
          "sidecar": true
        },
        {
          "args": true,
          "name": "bin/ffprobe",
          "sidecar": true
        },
        {
          "args": true,
          "name": "resources/spleeter",
          "sidecar": true
        }
      ]
    },
    "shell:allow-open",
    "dialog:allow-open",
    "dialog:allow-ask",
    "dialog:allow-message",
    "notification:default",
    "fs:default",
    "shell:default",
    "dialog:default",
    "notification:default",
    "updater:default",
    "process:default",
    "process:allow-restart"
  ]
}
